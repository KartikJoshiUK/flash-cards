generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String
  category String

  @@id([id, category])
}

model Card {
  id          Int    @id @default(autoincrement())
  uid         String
  category    String
  title       String
  description String
  imageUrl    String
  colorCode   String
}
